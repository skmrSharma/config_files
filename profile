# ash shell configuration file, like bashrc is for bash.
# name it as .profile and keep in $HOME directory.
# plugins used are: pfetch,paleta.
export PATH=/mnt/c/Program\ Files/Sublime\ Text\ 3/:$PATH
alias subl=subl.exe
alias mpv="/mnt/c/Program\ Files/MPV/mpv.com"
# current palette is 5peter-dark
# cat ~/.cache/current-palette
export PF_INFO="ascii title os host kernel shell uptime pkgs memory"
pfetch
